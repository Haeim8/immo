# Gateway (récupéré depuis https://app.pinata.cloud/gateway)
NEXT_PUBLIC_PINATA_GATEWAY=gateway.pinata.cloud

# Solana Configuration
NEXT_PUBLIC_SOLANA_RPC=https://api.devnet.solana.com
